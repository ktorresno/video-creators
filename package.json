{
  "name": "video-creators",
  "version": "1.0.0",
  "description": "Backend to expose some RESTful APIs to publish and unpublish videos.",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf node_modules dist",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktorresno/video-creators.git"
  },
  "keywords": [
    "Node.js",
    "Express.js",
    "sequelize",
    "PostgreSQL"
  ],
  "author": "Karen Torres",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ktorresno/video-creators/issues"
  },
  "homepage": "https://github.com/ktorresno/video-creators#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "postgresql": "^0.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/validator": "^13.7.14",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
