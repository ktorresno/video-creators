{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {
      "no-unused-expression": true
    },
    "rules": {
      "max-line-length": [
        true,
        130
      ],
      "prefer-template": false,
      "import-name": false,
      "no-console": false,
      "object-literal-sort-keys": false
    },
    "rulesDirectory": []
  }